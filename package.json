{
  "name": "photoorganizer",
  "version": "1.0.0",
  "description": "Photo Organizer Tool which helps users to organize their photos and videos inside a folder.",
  "main": "dist/main/main.js",
  "type": "module",
  "scripts": {
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "tsc -p tsconfig.renderer.json",
    "build:preload": "tsc -p tsconfig.preload.json",
    "build": "npm run build:main && npm run build:renderer && npm run build:preload",
    "start": "electron .",
    "prestart": "npm run build",
    "test": "jest"
  },
  "author": "Akshay Navale",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "electron": "^37.3.1",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "electron-log": "^5.4.3"
  }
}
