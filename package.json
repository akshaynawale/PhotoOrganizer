{
  "name": "photoorganizer",
  "version": "1.0.0",
  "description": "Photo Organizer Tool which helps users to organize their photos and videos inside a folder.",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build:main": "webpack --config webpack.main.config.ts",
    "build:renderer": "webpack --config webpack.renderer.config.ts",
    "build:preload": "tsc -p tsconfig.preload.json",
    "build": "npm run build:main && npm run build:renderer && npm run build:preload",
    "start": "electron .",
    "prestart": "npm run build",
    "test": "jest"
  },
  "author": "Akshay Navale",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-server": "^4.7.1",
    "@types/webpack-env": "^1.18.8",
    "cross-env": "^10.1.0",
    "electron": "^37.3.1",
    "jest": "^30.0.5",
    "source-map-loader": "^5.0.0",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "electron-log": "^5.4.3"
  }
}
